# Set NODE_ENV to either development or production
# Setting it to production will block requests if they do not come from domains
# included in the whitelist array inside /src/common/middleWares.ts file
NODE_ENV=

# You can put the domain for your weather frontend here or put it in the whitelist
DOMAIN_WEATHER=

# Same as above but for OpenAI API
DOMAIN_AI=

# Put your prefered port, e.g., 3000, 5000, 8000
PORT=

# Put your Openweather API key here
API_OW_KEY_VALUE=

# Put your OpenAI API key here
API_OPENAI_KEY_VALUE=

# ipgeolocation.io api URL for getting ip info
API_IPGEO_URL='https://api.ipgeolocation.io/ipgeo'

# Put your ipgeolocation api key here
API_IPGEO_KEY_VALUE=

# Openweather URL for weather api
API_OW_WEATHER_URL='https://api.openweathermap.org/data/2.5/weather'

# Openweather URL for onecall api
API_OW_ONECALL_URL='https://api.openweathermap.org/data/3.0/onecall'


# db
# Required for node-postgres
PGHOST= # Your database host
PGPORT= # port
PGDATABASE= # name of the db
PGUSER= # user for the db
PGPASSWORD= # that user's password

# secrets
JWT_SECRET= # you need a random string here

# superuser
SUPERUSERS= # array of user id you want to exempt from rate limiter: [34, 35]